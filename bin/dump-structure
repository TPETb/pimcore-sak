#!/bin/sh
echo "Creating class migrations... "
mkdir ../../data
chmod 0777 ../../data
retval=`php ../tpetb/pimcore-sak/src/MigrateCommit.php; echo $?`
if [ $retval -ne 0 ]; then
        echo "Migration failed.";
        exit $retval;
fi
git add ../../data/classes/
git add ../../data/fieldcollections/
echo "Done.\n"
